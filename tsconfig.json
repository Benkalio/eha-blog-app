{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "../eha-blog/components/Banner.tsx", "../eha-blog/components/Logo.tsx", "../eha-blog/components/Header.tsx", "components/ContactForm", "pages/api/posts/addComment.js", "pages/api/posts/[details].js", "pages/api/auth/[...nextauth].js"  ],
  "exclude": [
    "node_modules"
  ]
}
